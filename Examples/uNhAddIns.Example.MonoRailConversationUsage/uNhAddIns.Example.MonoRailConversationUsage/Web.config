<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="brail" type="Castle.MonoRail.Views.Brail.BrailConfigurationSection, Castle.MonoRail.Views.Brail"/>
  </configSections>
  <brail debug="false" saveToDisk="false" saveDirectory="Brail_Generated_Code" batch="false" commonScriptsDirectory="CommonScripts"/>
  <system.web>
    <httpHandlers>
      <add verb="*" path="*.rails" type="Castle.MonoRail.Framework.MonoRailHttpHandlerFactory, Castle.MonoRail.Framework"/>
      <add verb="*" path="*.brail" type="System.Web.HttpForbiddenHandler"/>
    </httpHandlers>
    <compilation debug="true"/>
    <customErrors mode="Off"></customErrors>
  </system.web>
</configuration>