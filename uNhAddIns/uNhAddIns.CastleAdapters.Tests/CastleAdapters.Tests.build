<?xml version="1.0" encoding="utf-8" ?>
<Project DefaultTargets="_Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="3.5">
	
	<Target Name="_Init" DependsOnTargets="CommonInit">
		<PropertyGroup>
			<ProjectFile>uNhAddIns.CastleAdapters.Tests.csproj</ProjectFile>
			<AssemblyTitle>uNhAddIns.CastleAdapters.Tests</AssemblyTitle>
			<AssemblyProduct>uNhAddIns.CastleAdapters.Tests</AssemblyProduct>
			<AssemblyDescription></AssemblyDescription>
			<AssemblyClsCompliant>True</AssemblyClsCompliant>
			<SkipTest>True</SkipTest>
		</PropertyGroup>
	</Target>
	
	<Target Name="_Build" DependsOnTargets="_Init;CommonCompileTest" Outputs="@(TestAssemblies)"/>
	
	<Import Project="$(RootDir)\BuildCommon\CommonProject.build"/>
</Project>


